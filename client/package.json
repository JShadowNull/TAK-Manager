{
  "name": "tak-manager-app",
  "productName": "Tak Manager",
  "version": "2.1.1",
  "description": "This is a modular Flask-based application that monitors CPU, RAM usage, and running services. It provides a simple interface for viewing system metrics using a Flask backend and a PyWebview frontend.",
  "type": "module",
  "main": "src/index.tsx",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "image:mac": "rm -rf ../tak-manager-wrapper/docker/* && docker build -t tak-manager:${npm_package_version} -f ../DockerfileProd .. && mkdir -p ../tak-manager-wrapper/docker && docker save tak-manager:${npm_package_version} | gzip > ../tak-manager-wrapper/docker/tak-manager-${npm_package_version}.tar.gz",
    "image:win": "docker build -t tak-manager:%npm_package_version% -f ..\\DockerfileProd .. && if not exist ..\\tak-manager-wrapper\\docker mkdir ..\\tak-manager-wrapper\\docker && docker save tak-manager:%npm_package_version% -o ..\\tak-manager-wrapper\\docker\\tak-manager-%npm_package_version%.tar",
    "docker:dev": "docker compose -f ../docker-compose.dev.yml --env-file ../.env.dev up"
  },
