  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "watch:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/styles.css --watch",
    "dev": "concurrently \"npm run start\" \"npm run watch:css\" --kill-others --handle-input",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 -h 0.0.0.0",
    "build-storybook": "storybook build"
